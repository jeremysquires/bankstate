{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: Module Handler",
      "type": "python",
      "request": "launch",
      "module": "src.handler"
    },   
    {
      "name": "Python: Current File",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "envFile": "${workspaceFolder}/.env",
      "cwd": "${fileDirname}"
    }
  ]
}